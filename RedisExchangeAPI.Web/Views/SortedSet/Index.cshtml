@model HashSet<string>


<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">

    <input type="text" name="name" /> <!--Key kısmı-->

    <input type="number" name="score" /> <!--Value kısmı-->

    <button type="submit">Kaydet</button>
</form>

<h1 style="margin-top: 3px;">Caclediğimiz değerleri Gösteren Liste</h1>
@foreach (var item in Model)
{
    <h2 style="margin-top: 2px;">@item <a asp-action="DeleteItem" asp-route-name="@item">Sil</a></h2>
    // name ismi DeleteItem(string name) kısmındaki değişken name'dir.'
}